{
  "extends": [
    "env"
  ],
  "rules": {
    "arrow-body-style": [
      "error",
      "always"
    ],
    "curly": [
      "error",
      "all"
    ],
    "no-sparse-arrays": "off",
    "react/react-in-jsx-scope": "off",
    "jsdoc/require-jsdoc": "off"
  },
  "parserOptions": {
    "sourceType": "module"
  }
}